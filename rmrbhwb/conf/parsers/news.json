{
  "name": "news",
  "example_url": "http://paper.people.com.cn/rmrbhwb/html/2021-03/07/content_2037028.htm",
  "default_fields": true,
  "zip_content": true,
  "rules": {
    "root": [
      {
        "type": "text",
        "key": "title",
        "xpath": "//h1"
      },
      {
        "type": "text",
        "key": "sub_title",
        "xpath": "//h2"
      },
      {
        "type": "text",
        "key": "author",
        "xpath": "//p[@class='sec']/text()"
      },
      {
        "type": "text",
        "key": "date",
        "xpath": "//div[@class='article']//span[@class='date']"
      },
      {
        "type": "text",
        "key": "content",
        "xpath": "//div[@id='ozoom']"
      }
    ]
  },
  "js": ""
}
